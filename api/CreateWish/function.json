{
  "bindings": [
    {
      "authLevel": "function",
      "type": "HttpTrigger",
      "direction": "in",
      "name": "Request",
      "methods": [
        "get, post"
      ] 
    },
    {
      "type": "http",
      "direction": "out",
      "name": "Response"
    },
    {
      "type": "cosmosDB",
      "name": "CosmosOutput",
      "direction": "out",
      "connectionStringSetting": "Festive_CosmosDB",
      "databaseName": "festivetechcalendar",
      "collectionName": "whishes"
    }    
  ]
}
